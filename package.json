{
  "name": "sharist-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@hookform/resolvers": "^1.2.0",
    "@reach/router": "^1.3.4",
    "axios": "^0.20.0",
    "i": "^0.3.6",
    "joi": "^17.3.0",
    "npm": "^6.14.10",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hook-form": "^6.13.1",
    "react-icons": "^4.1.0",
    "react-scripts": "^3.4.4",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/googlemaps": "^3.43.0",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.19.11",
    "@types/reach__router": "^1.3.6",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/styled-components": "^5.1.7",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "start": "BROWSER=.start-script.js PORT=8000 react-scripts start",
    "build": "react-scripts build && echo '/* /index.html 200' > build/_redirects",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "jsxBracketSameLine": false,
    "jsxSingleQuote": true,
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  }
}
